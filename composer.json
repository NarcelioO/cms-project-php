{
    "name": "narce/demo-aceda",
    "description": "PHP CMS Aplication",
    "autoload": {
        "psr-4": {
            "app\\": "app/",
            "core\\": "core/"
        }
    },
    "authors": [
        {
            "name": "Narcelio",
            "email": "narcelioferdffgh@gmail.com"
        }
    ],
    "scripts": {
        "serve" : [
        "Composer\\Config::disableProcessTimeout",
        "php -S localhost:8000 -t public"
        ]
    },
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "doctrine/dbal": "^4.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
